[
 { 
    "Name": "Loading Protocol Buffers to user/json blob format into EMR", 
    "ActionOnFailure": "CONTINUE", 
    "HadoopJarStep": { 
      "Jar": "/home/hadoop/contrib/streaming/hadoop-streaming.jar", 
      "Args": [ 
